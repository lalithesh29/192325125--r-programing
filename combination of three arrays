array1 <- matrix(1:9, nrow = 3, byrow = TRUE)
array2 <- matrix(10:18, nrow = 3, byrow = TRUE)
array3 <- matrix(19:27, nrow = 3, byrow = TRUE)
combined_array1 <- rbind(array1[1, ], array2[1, ], array3[1, ])
print(combined_array1)

output
 [,1] [,2] [,3]
[1,]    1    2    3
[2,]   10   11   12
[3,]   19   20   21
